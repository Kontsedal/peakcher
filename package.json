    "build:dev": "cross-env NODE_ENV=development webpack --config webpack/dev.js --watch",
    "build:prod": "cross-env NODE_ENV=production  webpack --config webpack/prod.js",
    "lint": "eslint ./src/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kontsedal/peakcher.git"
  },
  "author": "kontsedal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kontsedal/peakcher/issues"
  },
  "homepage": "https://github.com/Kontsedal/peakcher#readme",
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-extension-reloader": "^1.1.4",
    "webpack-merge": "^4.2.2",
    "copy-webpack-plugin": "^5.1.1",
    "html-webpack-plugin": "^4.2.0",
    "@types/chrome": "0.0.104"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.5",
    "@types/react-redux": "^7.1.7",
    "@types/redux": "^3.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.j|tsx?": "eslint --cache --fix",
    "*.{j|tsx?,s?css,md}": "prettier --write"
  }
}
