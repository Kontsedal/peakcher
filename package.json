{
  "name": "peakcher",
  "version": "2.0.1",
  "description": "Browser extension to store images in dropbox",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack --config webpack/dev.js --watch --progress",
    "build": "cross-env NODE_ENV=production  webpack --config webpack/prod.js --progress",
    "lint:js": "tsc --noEmit && eslint './src/**/*.?(tsx|ts)'",
    "lint:js:fix": "npm run lint -- --fix",
    "lint:style": "stylelint \"**/*.scss\"",
    "lint:style:fix": "stylelint \"**/*.scss\" --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:js:fix && npm run lint:style:fix",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "i18n:find-unused": "node scripts/findUnusedTranslations.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kontsedal/peakcher.git"
  },
  "author": "kontsedal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kontsedal/peakcher/issues"
  },
  "homepage": "https://github.com/Kontsedal/peakcher#readme",
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-typescript": "7.10.4",
    "@storybook/addon-actions": "6.0.26",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.0.26",
    "@storybook/addons": "6.0.26",
    "@storybook/react": "6.0.26",
    "@svgr/webpack": "5.4.0",
    "@types/chrome": "0.0.124",
    "@types/jest": "26.0.14",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/redux": "3.6.31",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.2.1",
    "cross-env": "7.0.2",
    "css-loader": "5.0.0",
    "dotenv": "8.2.0",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "11.0.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "file-loader": "6.1.1",
    "glob": "7.1.6",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "jest": "26.5.3",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "react-docgen-typescript-loader": "3.7.2",
    "sass": "1.27.0",
    "sass-loader": "10.0.3",
    "shelljs": "0.8.4",
    "signale": "1.4.0",
    "style-loader": "2.0.0",
    "stylelint": "13.7.2",
    "stylelint-config-sass-guidelines": "7.1.0",
    "stylelint-config-standard": "20.0.0",
    "ts-loader": "8.0.5",
    "typescript": "4.0.3",
    "webpack": "5.1.0",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "4.0.0",
    "webpack-merge": "5.2.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.4.0",
    "axios": "0.20.0",
    "classnames": "2.2.6",
    "lodash": "4.17.20",
    "mime": "2.4.6",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.13.1",
    "react-icons": "3.11.0",
    "react-redux": "7.2.1",
    "react-transition-group": "4.4.1",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "reselect": "4.0.0",
    "uuid": "8.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.j|tsx?": "eslint --cache --fix",
    "*.{j|tsx?,s?css,md}": "prettier --write"
  }
}
